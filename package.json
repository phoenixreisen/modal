    "pretest": "babel src --out-dir test",
    "test": "ospec --require ./test-setup.js",
    "posttest": "del-cli test"
  },
  "author": "Phoenix Reisen <it@phoenixreisen.com>",
  "contributors": [
    "Phoenix Reisen <it@phoenixreisen.com>",
