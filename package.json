  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/phoenixreisen/modal.git"
  },
  "license": "ISC",
  "dependencies": {
    "mithril": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-inline-svg": "^1.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^3.0.0",
    "jsdom": "^15.2.1",
    "mithril-node-render": "^2.3.2",
    "mithril-query": "^3.0.1",
    "ospec": "^4.0.1"
